<Window x:Class="Elden_Ring_Debug_Tool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Elden_Ring_Debug_Tool"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded" Closing="Window_Closing">
    <Window.DataContext>
        <local:ERViewModel x:Name="ViewModel"/>
    </Window.DataContext>
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Label Content="First Int:"/>
            <Label Content="{Binding Hook.PlayerInventoryFirstInt}"/>
        </StackPanel>
    </Grid>
</Window>
